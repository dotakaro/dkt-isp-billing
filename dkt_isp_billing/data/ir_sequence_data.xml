<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Sequence untuk ID Pelanggan -->
        <record id="seq_isp_customer" model="ir.sequence">
            <field name="name">Customer ID Sequence</field>
            <field name="code">isp.customer.sequence</field>
            <field name="prefix">CUS/%(year)s/</field>
            <field name="padding">5</field>
        </record>

        <!-- Sequence untuk Riwayat Perangkat -->
        <record id="seq_isp_device_history" model="ir.sequence">
            <field name="name">Device History Sequence</field>
            <field name="code">isp.device.history.sequence</field>
            <field name="prefix">DEV/%(year)s/</field>
            <field name="padding">5</field>
        </record>

        <!-- Sequence untuk Biaya Instalasi -->
        <record id="seq_isp_installation_fee" model="ir.sequence">
            <field name="name">Installation Fee Sequence</field>
            <field name="code">isp.installation.fee.sequence</field>
            <field name="prefix">INS/%(year)s/</field>
            <field name="padding">5</field>
        </record>

        <!-- Sequence untuk Subscription -->
        <record id="seq_isp_subscription" model="ir.sequence">
            <field name="name">ISP Subscription</field>
            <field name="code">isp.subscription.sequence</field>
            <field name="prefix">SUB/%(year)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo> 